{
    "version": 2,
    "builds": [
        { "src": "package.json", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/app.js"
      }
    ],
    "crons": [
        {
            "path": "/cronTask",
            "schedule": "00 15 * * 1"
        }
    ]
  }